{
   "background": {
      "scripts": [ "js/constants.js", "js/libs/jquery-3.1.1.min.js", "js/background.js" ]
   },
   "browser_action": {
      "default_icon": "assets/icons/icons_128.png",
      "default_popup": "popup.html",
      "default_title": "Message Cleaner"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/constants.js", "js/utils.js", "js/userinfo.js", "js/content.js" ],
      "matches": [ "*://*/*" ]
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self' ",
   "default_locale": "en",
   "description": "__MSG_app_description__",
   "homepage_url": "https://g-face.site/delete-all-messages/",
   "icons": {
      "128": "assets/icons/icons_128.png",
      "16": "assets/icons/icons_16.png",
      "48": "assets/icons/icons_48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_app_name__",
   "options_page": "settings.html",
   "permissions": [ "webRequest", "webRequestBlocking", "tabs", "storage", "*://*/*", "https://www.facebook.com/*", "https://web.facebook.com/*", "https://facebook.com/*", "http://facebook.com/*", "https://m.facebook.com/*", "http://www.facebook.com/*", "http://m.facebook.com/*", "http://mbasic.facebook.com/*", "https://mbasic.facebook.com/*" ],
   "short_name": "__MSG_app_short_name__",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.7.5",
   "web_accessible_resources": [ "assets/*.*", "assets/icon.png" ]
}
